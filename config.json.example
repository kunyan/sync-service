{
  "host": "spandx.dev",
  "port": 8001,
  "webroot": "/var/www/spandx",
  "autosync_targets": [
    {
      "interval": 10,
      "source": {
        "url": "https://access.redhat.com/services/chrome/head",
        "sub-paths": ["en", "kr", "ja", "zh_CN"]
      },
      "dest": {
        "path": "/var/www/spandx/chrome/head",
        "filename": "head.html"
      }
    },
    {
      "interval": 10,
      "source": {
        "url": "https://access.redhat.com/services/chrome/header",
        "sub-paths": ["en", "kr", "ja", "zh_CN"]
      },
      "dest": {
        "path": "/var/www/spandx/chrome/header",
        "filename": "header.html"
      }
    },
    {
      "interval": 10,
      "source": {
        "url": "https://access.redhat.com/services/chrome/footer",
        "sub-paths": ["en", "kr", "ja", "zh_CN"]
      },
      "dest": {
        "path": "/var/www/spandx/chrome/footer",
        "filename": "footer.html"
      }
    }
  ]
}
